#Rock paper scissor


import random
#top
h=int(input("choose 0 for rock, 1 for paper,2 for scissor"))
rock=0
paper=1
scissor=2
c=random.randint(0,2)
if(c==0 and h==1):
 print("you win")
elif(c==0 and h==0):
  print("tie")
elif(c==0 and h==2):
  print("you lose")
elif(c==1 and h==0):
  print("you lose")
elif(c==1 and h==1):
  print("tie")
elif(c==1 and h==2):
  print("you win")
elif(c==2 and h==0):
  print("you win")
elif(c==2 and h==1):
  print("you lose")
else:
  print("tie")
